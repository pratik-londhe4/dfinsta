{
	"manifest_version": 2,
	"name": "DFInsta",
	"version": "1.0",
	"description": "Hide suggested posts on Instagram",
	"permissions": ["activeTab"],
	"browser_action": {
	  "default_popup": "popup.html"
	},
	"content_scripts": [
	  {
		"matches": ["*://www.instagram.com/*"],
		"js": ["content.js"]
	  }
	]
  }
  